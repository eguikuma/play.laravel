{
  "$schema": "https://getcomposer.org/schema.json",
  "type": "project",
  "description": "A minimal Laravel project setup.",
  "keywords": [
    "laravel",
    "framework",
    "docker"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Examples\\Providers\\": "examples/providers/",
      "Examples\\Models\\": "examples/models/",
      "Examples\\Components\\": "examples/components/",
      "Examples\\Users\\UseCases\\": "examples/users/use-cases/",
      "Examples\\Users\\Requests\\": "examples/users/requests/",
      "Examples\\Users\\Endpoints\\": "examples/users/endpoints/",
      "Examples\\Users\\Components\\": "examples/users/components/",
      "Examples\\Users\\Fixtures\\": "examples/users/fixtures/",
      "Examples\\Users\\Tests\\": "examples/users/tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/telescope"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "doctrine/dbal": "^4.4",
    "guzzlehttp/guzzle": "^7.10",
    "laravel/framework": "^12.43",
    "livewire/livewire": "^3.7"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^3.6",
    "fakerphp/faker": "^1.24",
    "itsgoingd/clockwork": "^5.3",
    "larastan/larastan": "^3.8",
    "laravel-lang/lang": "^15.26",
    "laravel/pint": "^1.26",
    "laravel/telescope": "^5.16",
    "laravel/tinker": "^2.10",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.8",
    "nunomaduro/phpinsights": "^2.13",
    "phpunit/phpunit": "^12.5",
    "spatie/laravel-ignition": "^2.9"
  }
}
